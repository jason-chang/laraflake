{
    "name": "hafael/laraflake",
    "description": "Laravel Package based on Twitter Snowflake ID Generator (64 bit ID).",
    "keywords": ["laravel", "id", "shard", "64bit", "shard", "snowflake"],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "orchestra/testbench": "~3.0",
        "laravel/framework": "~5.3"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rafael Villa-Verde",
            "email": "villa655321verde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hafael\\LaraFlake\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/LaraFlakeTest.php"
        ],
        "psr-4": {
            "Hafael\\LaraFlake\\": "packages/hafael/laraflake/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
